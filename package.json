{
  "name": "netmaker-dashboard",
  "private": true,
  "version": "0.22.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "predev": "node scripts/resolve-asset-path.cjs",
    "postdev": "node scripts/reset-asset-path.cjs",
    "prebuild": "node scripts/resolve-asset-path.cjs",
    "e2e:setup": "playwright install --with-deps",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@react-sigma/core": "^3.4.2",
    "antd": "~5.13",
    "axios": "^1.6.5",
    "axios-mock-adapter": "^1.22.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "graphology": "^0.25.4",
    "graphology-layout": "^0.6.1",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-types": "^0.24.7",
    "i18next": "^23.7.6",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "rc-virtual-list": "^3.10.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.2",
    "react-use-intercom": "^5.1.4",
    "react-window": "^1.8.10",
    "semver": "^7.5.4",
    "sigma": "^2.4.0",
    "swr": "^2.2.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "sass": "^1.69.4",
    "typescript": "^5.3.3",
    "vite": "^4.4.9",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
